                                    </li>
                                    <li>
                                        <a class="dropdown-item" href="~/Relatos/Index">Relatos</a>
                                    </li>
                                    <li>
                                        <a class="dropdown-item" href="~/Comuna2/Index">Comuna 2</a>
                                    </li>
                                    <li>
                                        <a class="dropdown-item" href="~/Comuna3/Index">Comuna 3</a>
                                    </li>
                                </ul>
                            </li>

                            <li>
                                <a class="dropdown-item dropdown-toggle" href="#">
                                    Gestión cultural
                                </a>
                                <ul class="dropdown-menu dropdown-submenu">
                                    <li>
                                        <a class="dropdown-item" href="~/GestionCultural/Index">Inicio</a>
                                    </li>
                                </ul>
                            </li>

                            <li>
                                <a class="dropdown-item dropdown-toggle" href="#">
                                    Movilización social
                                </a>
                                <ul class="dropdown-menu dropdown-submenu">
                                    <li>
                                        <a class="dropdown-item" href="~/MovilizacionSocial/Index">Inicio</a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </li>
                    <li><a href="~/Reseñas/Index">Reseñas</a></li>
                    <li><a href="~/Aliados/Index">Aliados</a></li>
                    <li><a href="~/contactanos/Index">Contáctanos</a></li>
                    @if (CommonUtils.Success)
                    {
                        <li><a class="btn-casa-relato" href="~/Iniciar_Sesion/CerrarSesion">Cerrar Sesión</a></li>
                    }
                    else
                    {

                        <li><a class="btn-casa-relato" href="~/Iniciar_Sesion/Index">Iniciar sesión</a></li>
                    }
                    

                </ul>
                <a href="/">
                    <img class="logo" src="~/images/logo.jpg" alt="logo de casa relato">
                </a>
            </div>
        </nav>
