    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted" style="background-color: white">
        <div class="container">
            <div class="row">
                <div class="col-2">
                    <img src="../images/logo.jpg" alt="Logo oficial" class="img-footer" />
                </div>
                <div class="col-3">
                    <ul class="footer-1">
                        <li class="t-footer-section">Sobre nosotros</li>
                        <li><a href="#">Nuestros valores</a></li>
                        <li><a href="#">Agenda de eventos</a></li>
                        <li><a href="#">Contacto</a></li>
